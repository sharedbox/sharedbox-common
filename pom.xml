    <maven.compiler.source>${java.version}</maven.compiler.source>
    <maven.compiler.target>${java.version}</maven.compiler.target>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
    <!-- Spring -->
    <srping.version>5.3.3</srping.version>
    <srping.data.version>2.5.0</srping.data.version>
    <spring.security.version>5.4.6</spring.security.version>
    <!-- Log4j -->
    <log4j.version>2.14.1</log4j.version>
  </properties>
  
  <organization>
    <name>SharedBox</name>
    <url>http://www.sharedbox.com.br</url>
  </organization>
  
  <developers>
    <developer>
      <name>Rafael Costi Roque</name>
      <email>rafaelcosti@outlook.com</email>
    </developer>
  </developers>  
  
